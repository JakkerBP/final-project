{% extends 'admin.html.twig' %}

{% block title %}Key{% endblock %}

{% block body %}
    <h1>Key</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ key.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ key.name }}</td>
            </tr>
            <tr>
                <th>Symbol</th>
                <td>{{ key.symbol }}</td>
            </tr>
            <tr>
                <th>KeyPreview</th>
                <td>{{ key.keyPreview }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admin_key_index') }}">back to list</a>

    <a href="{{ path('app_admin_key_edit', {'id': key.id}) }}">edit</a>

    {{ include('admin_key/_delete_form.html.twig') }}
{% endblock %}
